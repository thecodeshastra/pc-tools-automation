    Hostname github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/thecodeshastra_github
    IdentitiesOnly yes

Host gitlab-codeshastra
    User git
    Hostname gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/thecodeshastra_gitlab
    IdentitiesOnly yes
